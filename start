echo "1 ) Gõ số 1 nếu bạn chưa cài đặt php";
echo "2 ) Gõ số 2 nếu bạn đã cài đặt php";
echo "99) Gõ số 99 để thoát";
read -p "[K90 HGM] >"  act;

if [ $act = 1 ] || [ $act = 1 ]
then
apt update -y && apt upgrade -y
pkg install -y php
clear
sleep 1
php 1.php
clear
php 2.php
sleep 2
clear
php 3.php
fi

if [ $act = 2 ] || [ $act = 2 ]
then
apt update -y && apt upgrade -y
clear
php 1.php
clear
php 2.php
sleep 2
clear
php 3.php
fi

if [ $act = 99 ] || [ $act = 99 ]
then
echo -e $red'K90 HGM'
sleep 1
echo -e $red'K90 HGM'
sleep 2
echo -e $red'K90 HGM'
sleep 1
echo -e $red'(╥_╥)'
